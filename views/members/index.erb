
<table class="member_table">

<tr>
  <th>Membership Number</th>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Membership Type</th>
</tr>

<% @members.each do |member| %>
<tr>
    <td><%= member.id %></td>
    <td><a href="/members/<%= member.id %>"><%= member.first_name() %></a></td>
    <td><%= member.last_name %></td>
    <td> <%= member.membership_type %></td>
</tr>
<% end %>
</table>

<p><a href="/members/new">Create a New Member</a></p>
<a href="/">Home</a>
